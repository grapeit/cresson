export CRESSON_MYSQL_HOST=cresson-db
export CRESSON_MYSQL_PORT=3306
export CRESSON_MYSQL_ROOT_PASSWORD=your-mysql-password
export CRESSON_API_AUTH_SECRET=your-auth-secret
export CRESSON_API_DB_DRIVER=mysql
export CRESSON_API_DB_CONNECT="root:$CRESSON_MYSQL_ROOT_PASSWORD@tcp($CRESSON_MYSQL_HOST:$CRESSON_MYSQL_PORT)/cresson"
export CRESSON_API_DEBUG=1
